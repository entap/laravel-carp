{
    "name": "entap/laravel-carp",
    "description": "Laravel Package Manager API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ii masamitsu",
            "email": "ii@entap.co.jp"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^8.36",
        "illuminate/database": "^8.36",
        "illuminate/console": "^8.36",
        "illuminate/http": "^8.36",
        "illuminate/contracts": "^8.36"
    },
    "autoload": {
        "psr-4": {
            "Entap\\Laravel\\Carp\\": "src/",
            "Entap\\Laravel\\Carp\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entap\\Laravel\\Carp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Entap\\Laravel\\Carp\\CarpServiceProvider"
            ]
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.17",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit tests"
    }
}
